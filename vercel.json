{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "colleague.txt"
    }
  },
  "cron": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    { "src": "/api/cron", "dest": "/api/cron.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
